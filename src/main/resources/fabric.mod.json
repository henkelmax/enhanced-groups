{
  "schemaVersion": 1,
  "id": "enhancedgroups",
  "version": "${mod_version}",
  "name": "Simple Voice Chat Enhanced Groups",
  "description": "Instantly create and join groups",
  "authors": [
    "Max Henkel"
  ],
  "contact": {
    "homepage": "https://modrepo.de/"
  },
  "license": "ARR",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.maxhenkel.enhancedgroups.EnhancedGroups"
    ],
    "voicechat": [
      "de.maxhenkel.enhancedgroups.EnhancedGroupsVoicechatPlugin"
    ]
  },
  "depends": {
    "fabricloader": "${fabric_loader_dependency}",
    "minecraft": [
      "1.21",
      "1.21.1"
    ],
    "voicechat": ">=${minecraft_version}-${voicechat_api_version}"
  },
  "breaks": {
    "fabric-api": "${fabric_api_dependency_breaks}"
  }
}
